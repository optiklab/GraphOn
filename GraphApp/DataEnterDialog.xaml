<Window x:Class="GraphApp.WeightEnterDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Ввод данных"
        Height="120"
        Width="300"
        Loaded="Window_Loaded"
        Icon="../Decrease Decimal.ico"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel Name="StackDefault" Orientation="Vertical">
            <Label Name="Caption">Введите вес нового ребра:</Label>
            <TextBox Name="TextBox1"></TextBox>
            <Button Margin="0,5,0,0" Height="25" Width="100" Name="Okbutton" Click="Okbutton_Click">OK</Button>
        </StackPanel>
    </Grid>
</Window>
